<div class="animated fadeIn retraso">
    <div class="form-signin">
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <h2 class="form-signin-heading">Iniciar sesión</h2>
          <hr class="colorgraph"><br>
          <%= f.label :email %><br />
          <%= f.email_field :email, autofocus: true, class:"form-control", placeholder:"Correo" %>
          <%= f.label :password %><br />
          <%= f.password_field :password, autocomplete: "off", placeholder:"Contraseña", class:"form-control" %>
          <% if devise_mapping.rememberable? -%>
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me %>
          <%end%>
          <%= f.submit "Iniciar sesión", class:"btn btn-lg btn-primary btn-block" %>
          <br>
          <center>
            <%= render "devise/shared/links" %>
          </center>
        <%end%>
    </div>
</div>